# mount point	fstype    device    	[device2]

/boot       mtd         /dev/block/mmcblk0p20
/cache      yaffs2      /dev/block/mmcblk0p23
/data       yaffs2      /dev/block/mmcblk0p25
/recovery   mtd         /dev/block/mmcblk0p21
/system     yaffs2      /dev/block/mmcblk0p22
/sdcard     vfat        /dev/block/mmcblk0p1
/sdcard1    ext4        /dev/block/mmcblk0p2
